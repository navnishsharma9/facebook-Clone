{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi":"off",
        "no-unused-vars":"off",
        "space-infix-ops":"off",
        "no-multiple-empty-lines":"off"
    }
}
